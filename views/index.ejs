<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/head.ejs')%>
  </head>
  <body>
    <nav>
        <a href="/pokemon/new">Add a Pokemon</a>
    </nav>
    <h1>Pokemon Manager</h1>
    <ul>
      <% data.forEach(data => { %>
        <li>
          <a href="/pokemon/<%=data.id%>"> <%=data.name %></a><br>
          <img src=" <%= data.img %>" /><br>
          <a>Edit  </a><a>  Delete</a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>